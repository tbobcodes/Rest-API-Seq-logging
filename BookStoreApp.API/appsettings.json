{
  "ConnectionStrings": {
    "BookStoreDbAppConnection": "Server=.\\SQLEXPRESS;Database=BookAPI;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "JwtSettings": {
    "Issuer": "BookStoreAPI",
    "Audience": "BookStoreApiClient",
    "Duration": "1"
  },
  "AllowedHosts": "*"
}
